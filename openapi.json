  "paths": {
    "/ocr/identity/id_card": {
      "post": {
        "tags": [
          "證件"
        ],
        "summary": "中華民國國民身分證解析",
        "description": "Process an ID card image and extract structured information.",
        "operationId": "id_card",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/Body_id_card"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "title": "Response Id Card"
                },
                "example": {
                  "status": "success",
                  "data": {
                    "姓名": "王小慶",
                    "出生年月日": "民國81年10月21日",
                    "統一編號": "A123456789"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid file type",
            "content": {
              "application/json": {
                "example": {
                  "status": "error",
                  "message": "Invalid file type. Only PNG, JPG, and JPEG are allowed."
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "example": {
                  "status": "error",
                  "message": "Error message"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        },
        "security": [
          {
            "OAuth2PasswordBearer": []
          }
        ]
      }
    },
